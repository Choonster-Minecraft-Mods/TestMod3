{
  "multipart": [
    {
      "apply": {
        "model": "testmod3:block/fluid_pipe_centre",
        "uvlock": true
      }
    },
    {
      "apply": {
        "model": "testmod3:block/fluid_pipe_side",
        "uvlock": true,
        "x": 90
      },
      "when": {
        "down": "true"
      }
    },
    {
      "apply": {
        "model": "testmod3:block/fluid_pipe_side",
        "uvlock": true,
        "x": -90
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "testmod3:block/fluid_pipe_side",
        "uvlock": true
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "testmod3:block/fluid_pipe_side",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "testmod3:block/fluid_pipe_side",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "testmod3:block/fluid_pipe_side",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "true"
      }
    }
  ]
}